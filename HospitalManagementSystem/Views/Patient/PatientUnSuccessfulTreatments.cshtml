@model List<HospitalManagementSystem.ViewModels.PatientUnSuccessfulTreatmentsViewModel>

<h3>Patient Treatments</h3>

<table class="table">
    <thead>
        <tr>
            <th>Patient ID</th>
            <th>First Name</th>
            <th>Age</th>
            <th>Address</th>
            <th>Treatment Type</th>
            <th>Unsuccessful Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.PatientId</td>
                <td>@item.FirstName</td>
                <td>@item.Age</td>
                <td>@item.Address</td>
                <td>@item.TreatmentType</td>
                <td>@item.UnsuccessfulCount</td>
            </tr>
        }
    </tbody>
</table>
